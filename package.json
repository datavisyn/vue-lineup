{
  "name": "vue-lineup",
  "description": "LineUp is an interactive technique designed to create, visualize and explore rankings of items based on a set of heterogeneous attributes.",
  "homepage": "https://github.com/lineupjs/vue-lineup",
  "version": "4.1.3",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://www.sgratzl.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lineupjs/vue-lineup/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lineupjs/vue-lineup.git"
  },
  "main": "./dist/vue-lineup.umd.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf build dist \"*.tgz\" && yarn run clean:compile",
    "clean:compile": "rimraf build/src \"build/*.tsbuildinfo\" \"*.tsbuildinfo\" ",
    "compile": "tsc",
    "compile:dev": "tsc -p ./tsconfig.dev.json",
    "fix": "yarn run prettier:write",
    "prettier:write": "prettier \"*\" \"*/**\" --write",
    "prettier": "prettier \"*\" \"*/**\" --check",
    "start": "vue-cli-service serve",
    "build:dev": "vue-cli-service build --mode development --target lib src/index.ts",
    "lint": "vue-cli-service lint && yarn prettier",
    "build": "vue-cli-service build --target lib src/index.ts",
    "test": "echo 'no tests'",
    "prepublishOnly": "yarn run build",
    "release": "release-it --disable-metrics --npm.skipChecks",
    "release:pre": "release-it --disable-metrics --npm.skipChecks --preRelease=alpha --npm.tag=next"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@yarnpkg/pnpify": "^2.4.0",
    "prettier": "^2.2.1",
    "release-it": "^14.4.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.5",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "lineupjs": "~4.1.2",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2"
  },
  "browserslist": [
    "Firefox ESR",
    "last 2 Firefox versions",
    "last 2 Chrome versions"
  ]
}
