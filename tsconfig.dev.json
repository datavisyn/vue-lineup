{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es6"
  },
  "typedocOptions": {
    "name": "LineUpJS",
    "out": "./docs",
    "theme": "default",
    "excludeExternals": true,
    "excludePrivate": true,
    "includeVersion": true,
    "hideGenerator": true,
    "exclude": "**/*.(d.ts|js|spec.ts|scss)",
    "plugin": ["typedoc-plugin-as-member-of"]
  }
}
